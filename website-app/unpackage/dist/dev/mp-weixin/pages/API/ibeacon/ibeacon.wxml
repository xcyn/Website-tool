<view><page-head vue-id="a9a50a4e-1" title="{{title}}" bind:__l="__l"></page-head><view class="uni-padding-wrap uni-common-mt"><view class="uni-btn-v"><button type="primary" disabled="{{isOpen}}" data-event-opts="{{[['tap',[['openBluetoothAdapter',['$event']]]]]}}" bindtap="__e">打开蓝牙模块</button><button type="primary" disabled="{{!isOpen}}" data-event-opts="{{[['tap',[['closeBluetoothAdapter',['$event']]]]]}}" bindtap="__e">关闭蓝牙模块</button><button type="primary" disabled="{{!isOpen||isStarted}}" loading="{{isStarted}}" data-event-opts="{{[['tap',[['startBeaconDiscovery',['$event']]]]]}}" bindtap="__e">开始搜索附近的iBeacon设备</button><button type="primary" disabled="{{!isStarted}}" data-event-opts="{{[['tap',[['stopBeaconDiscovery',['$event']]]]]}}" bindtap="__e">停止搜索附近的iBeacon设备</button></view></view><scroll-view class="uni-scroll_box"><block wx:if="{{isStarted||deviceList.length>0}}"><view class="uni-title">{{"已经发现 "+deviceList.length+" 台设备:"}}</view></block><block wx:for="{{deviceList}}" wx:for-item="item" wx:for-index="index" wx:key="uuid"><view class="uni-list-box"><view><view class="uni-list_name">{{"UUID: "+item.uuid}}</view><view class="uni-list_item">{{"major: "+item.major}}</view><view class="uni-list_item">{{"minor: "+item.minor}}</view><view class="uni-list_item">{{"rssi: "+item.rssi+" dBm"}}</view><view class="uni-list_item">{{"accuracy: "+item.accuracy}}</view><view class="uni-list_item">{{"heading: "+item.heading}}</view></view></view></block></scroll-view></view>